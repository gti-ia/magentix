<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE uridef [
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY service "http://www.daml.org/services/owl-s/1.1/Service.owl#">
  <!ENTITY profile "http://www.daml.org/services/owl-s/1.1/Profile.owl#">
  <!ENTITY process "http://www.daml.org/services/owl-s/1.1/Process.owl#">
  <!ENTITY grounding "http://www.daml.org/services/owl-s/1.1/Grounding.owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY mind "http://localhost:8080/sfservices/THservices/owl/owls/SearchAccommodationProfile.owl">
  <!ENTITY actor "http://www.daml.org/services/owl-s/1.1/ActorDefault.owl#">
  <!ENTITY role "http://localhost:8080/sfservices/THservices/owl/owls/role.owl">
  <!ENTITY list "http://www.daml.org/services/owl-s/1.2/generic/ObjectList.owl#">
  <!ENTITY swrl "http://www.w3.org/2003/11/swrl#">
  <!ENTITY role_param "http://localhost:8080/sfservices/THservices/owl/owls/role_param.owl#">
]>


<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:owl="&owl;" xmlns:xsd="&xsd;#" 
   xmlns:service="&service;" xmlns:process="&process;" xmlns:profile="&profile;" xmlns:grounding="&grounding;" xmlns:role="&role;#" xmlns:actor="&actor;" xmlns:list="&list;" xmlns:swrl="&swrl;" xmlns:role_param="&role_param;"
   xmlns:mind="&mind;#" >

<profile:Profile rdf:ID="SearchAccommodationProfile">
    <service:presentedBy rdf:resource="#SearchAccommodationService"/>
    <profile:serviceName xml:lang="en">SearchCheapHotel</profile:serviceName>
    <profile:textDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Searchs a Hotel</profile:textDescription>
    <profile:hasInput rdf:resource="#SearchAccommodationInputCategory"/>
    <profile:hasInput rdf:resource="#SearchAccommodationInputCountry"/>
    <profile:hasInput rdf:resource="#SearchAccommodationInputCity"/>
    <profile:hasOutput rdf:resource="#SearchAccommodationOutputHotelCompany"/>
    <profile:hasOutput rdf:resource="#SearchAccommodationOutputHotel"/>
<role_param:hasLocal>
   <role:role_list rdf:ID="provider_list">
            <role:role_atom rdf:ID="Agency"/>  
	    <role:role_atom rdf:ID="TravelProvider"/>  	
	    <role:role_atom rdf:ID="TourProvider"/>   
</role:role_list>
</role_param:hasLocal>

<role_param:hasLocal>
   <role:role_list rdf:ID="client_list">
            <role:role_atom rdf:ID="Tourist"/>  
	    <role:role_atom rdf:ID="Traveler"/>  	   
</role:role_list>
</role_param:hasLocal>
</profile:Profile>
</rdf:RDF> 

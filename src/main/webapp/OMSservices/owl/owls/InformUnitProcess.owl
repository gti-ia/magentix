<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE uridef [
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY service "http://www.daml.org/services/owl-s/1.1/Service.owl#">
  <!ENTITY profile "http://www.daml.org/services/owl-s/1.1/Profile.owl#">
  <!ENTITY process "http://www.daml.org/services/owl-s/1.1/Process.owl#">
  <!ENTITY grounding "http://www.daml.org/services/owl-s/1.1/Grounding.owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY mind "http://localhost:8080/OMS/OWLS/InformUnit.owl">
  <!ENTITY dir "http://localhost:8080/omsservices/services/InformUnit?wsdl">
]>


<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:owl="&owl;" xmlns:xsd="&xsd;#" 
   xmlns:service="&service;" xmlns:process="&process;" xmlns:profile="&profile;" xmlns:grounding="&grounding;"
   xmlns:mind="&mind;#"      
   xml:base="http://localhost:8080/OMS/OWLS/InformUnit.owl">


<owl:Ontology>
    <owl:imports rdf:resource="&service;"/>
    <owl:imports rdf:resource="&profile;"/>
    <owl:imports rdf:resource="&process;"/>
    <owl:imports rdf:resource="&grounding;"/> 
</owl:Ontology>

<service:Service rdf:ID="InformUnitService">
        <service:presents rdf:resource="#InformUnitProfile"/>
        <service:describedBy rdf:resource="#InformUnitProcess"/>
	<service:supports rdf:resource="#InformUnitGrounding"/>
</service:Service>


       

<profile:Profile rdf:ID="InformUnitProfile">
    <service:isPresentedBy rdf:resource="#InformUnitService"/>
    <profile:serviceName xml:lang="en">InformUnit</profile:serviceName>



    <profile:hasInput rdf:resource="#UnitID"/>
    <profile:hasInput rdf:resource="#AgentID"/>
    <profile:hasOutput rdf:resource="#Status"/>
    <profile:hasOutput rdf:resource="#ErrorValue"/>   
    <profile:hasOutput rdf:resource="#UnitType"/>
    <profile:hasOutput rdf:resource="#UnitGoal"/> 
    <profile:hasOutput rdf:resource="#ParentID"/> 

</profile:Profile>

<process:AtomicProcess rdf:ID="InformUnitProcess">
	<service:describes rdf:resource="#InformUnitService"/>  	

    <process:hasInput rdf:resource="#UnitID"/>
	<process:hasInput rdf:resource="#AgentID"/>
	<process:hasOutput rdf:resource="#Status"/>
	<process:hasOutput rdf:resource="#ErrorValue"/>
	<process:hasOutput rdf:resource="#UnitType"/>
    <process:hasOutput rdf:resource="#UnitGoal"/> 
    <process:hasOutput rdf:resource="#ParentID"/> 
	
</process:AtomicProcess>


<process:Input rdf:ID="UnitID">
     <rdfs:label>UnitID</rdfs:label>
     <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Input>
<process:Input rdf:ID="AgentID">
     <rdfs:label>AgentID</rdfs:label>
     <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Input>

<process:Output rdf:ID="Status">
    <rdfs:label>Status</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#integer</process:parameterType>
</process:Output>  
<process:Output rdf:ID="ErrorValue">
    <rdfs:label>ErrorValue</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Output>    
<process:Output rdf:ID="UnitType">
    <rdfs:label>UnitType</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Output>
<process:Output rdf:ID="UnitGoal">
    <rdfs:label>UnitGoal</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Output>
<process:Output rdf:ID="ParentID">
    <rdfs:label>ParentID</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#string</process:parameterType>
</process:Output>      

<grounding:WsdlGrounding rdf:ID="InformUnitGrounding">
	<service:supportedBy rdf:resource="#InformUnitService"/>
	<grounding:hasAtomicProcessGrounding rdf:resource="#InformUnitWsdlAtomicProcessGrounding"/>
</grounding:WsdlGrounding>

<grounding:WsdlAtomicProcessGrounding rdf:ID="InformUnitWsdlAtomicProcessGrounding">
	<grounding:owlsProcess rdf:resource="#InformUnitProcess"/>
	<grounding:wsdlDocument rdf:datatype="&xsd;anyURI">http://localhost:8080/omsservices/services/InformUnit?wsdl</grounding:wsdlDocument>
	<grounding:wsdlOperation>
            <grounding:WsdlOperationRef>
                <grounding:portType rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnitOperationsPortType</grounding:portType>
                <grounding:operation rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit</grounding:operation>
            </grounding:WsdlOperationRef>
        </grounding:wsdlOperation>

	<grounding:wsdlInputMessage rdf:datatype="&xsd;anyURI">http://localhost:8080/omsservices/services/InformUnitMessage</grounding:wsdlInputMessage>
        <grounding:wsdlOutputMessage rdf:datatype="&xsd;anyURI">http://localhost:8080/omsservices/services/InformUnitResponse</grounding:wsdlOutputMessage>
    
	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#UnitID"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/UnitID</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>
	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#AgentID"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/AgentID</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

       <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#Status"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/Status</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>

<grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#ErrorValue"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/ErrorValue</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>
        <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#UnitType"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/UnitType</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>
        <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#UnitGoal"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/UnitGoal</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>
        <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#ParentID"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/omsservices/services/InformUnit/ParentID</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>        

</grounding:WsdlAtomicProcessGrounding>

</rdf:RDF> 

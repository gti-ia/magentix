<assembly>
	<id>jar-with-dependencies</id>
	<baseDirectory>/</baseDirectory>
	<moduleSets>
		<moduleSet>
			<includes>
				<include>org.apache.axis2:addressing</include>
			</includes>
		</moduleSet>
	</moduleSets>
	<dependencySets>
		<dependencySet>
			<unpack>true</unpack>
			<outputDirectory></outputDirectory>
			
				<excludes>
					<exclude>org.apache.axis2:addressing</exclude>
					<exclude>org.apache.rampart:rampart-core</exclude>
					<exclude>org.apache.rampart:rampart-policy</exclude>
					<exclude>org.bouncycastle:bcprov-ext-jdk15</exclude>
					<exclude>org.mozilla:jss</exclude>
					<exclude>bouncycastle:*:jar:*</exclude>
				</excludes>

		</dependencySet>
	</dependencySets>
	<formats>
		<!-- Formatos en el que queremos el zip -->
		<format>zip</format>
	</formats>
</assembly>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>es.upv.dsic.gti-ia</groupId>
	<artifactId>MMS</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>aar</packaging>

	<name>MMS</name>
	<url>http://maven.apache.org</url>

	<build>
		<finalName>MMS</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.axis2</groupId>
				<artifactId>axis2-aar-maven-plugin</artifactId>
				<version>1.4.1</version>
				<extensions>true</extensions>
				<configuration>
					<fileSets>
						<fileSet>
							<directory>
								src/main/config/  
                             </directory>
							<outputDirectory>META-INF</outputDirectory>
							<includes>
								<include>services.xml</include>
							</includes>
						</fileSet>
					</fileSets>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>org.apache.ws.security</groupId>
			<artifactId>wss4j</artifactId>
			<version>1.5.8</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>bouncycastle</groupId>
			<artifactId>bcprov-jdk16</artifactId>
			<version>140</version>
		</dependency>
		<dependency>
			<groupId>org.apache.axis2</groupId>
			<artifactId>axis2-jaxws</artifactId>
			<version>1.5.1</version>
		</dependency>
			<dependency>
			<groupId>org.apache</groupId>
			<artifactId>qpid-management-console-seguridad</artifactId>
			<version>0.7</version>
		</dependency>
			<dependency>
			<groupId>org.apache</groupId>
			<artifactId>qpid-client-seguridad</artifactId>
			<version>0.7</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.apache</groupId>
			<artifactId>qpid-common-seguridad</artifactId>
			<version>0.7</version>
		</dependency>
			<dependency>
			<groupId>javax.jms</groupId>
			<artifactId>jms</artifactId>
			<version>1.1</version>
		</dependency>
		<dependency>
			<groupId>org.apache.mina</groupId>
			<artifactId>mina-core</artifactId>
			<version>1.0.0</version>
			<scope>runtime</scope>
		</dependency>
	</dependencies>
</project> 

<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:owl="&owl;" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
   xmlns:service="&service;" xmlns:process="&process;" xmlns:profile="&profile;" xmlns:grounding="&grounding;"
   xmlns:mind="&mind;#"      
   xmlns:provider = "http://localhost:8080/ontologies/provider.owl#"
   xml:base="http://localhost:8080/testSFservices/testSFservices/owl/owls/Division.owl">

<owl:Ontology>
    <owl:imports rdf:resource="&service;"/>
    <owl:imports rdf:resource="&profile;"/>
    <owl:imports rdf:resource="&process;"/>
    <owl:imports rdf:resource="&grounding;"/> 
    <owl:imports rdf:resource="http://localhost:8080/ontologies/provider.owl" />
</owl:Ontology>

<service:Service rdf:ID="DivisionService">
        <service:presents rdf:resource="#DivisionProfile"/>
	<service:describedBy rdf:resource="#DivisionProcess"/>
	<service:supports rdf:resource="#DivisionGrounding"/>
	<service:supports rdf:resource="#DivisionGrounding2"/>
</service:Service>

<profile:Profile rdf:ID="DivisionProfile">
    <service:isPresentedBy rdf:resource="#DivisionService"/>
    <profile:serviceName xml:lang="en">Division</profile:serviceName>
	<profile:textDescription xml:lang="en">
	This service receives two numbers X and Y as inputs. Then, it calculates X divided by Y
	</profile:textDescription>
    <profile:hasInput rdf:resource="#x"/>
    <profile:hasInput rdf:resource="#y"/>
    <profile:hasOutput rdf:resource="#Result"/>
</profile:Profile>


<process:AtomicProcess rdf:ID="DivisionProcess">
	<service:describes rdf:resource="#DivisionService"/>
     	<process:hasInput rdf:resource="#x"/>
	<process:hasInput rdf:resource="#y"/>
	<process:hasOutput rdf:resource="#Result"/>
</process:AtomicProcess>


<process:Input rdf:ID="x">
     <rdfs:label>x</rdfs:label>
     <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/2001/XMLSchema#double</process:parameterType>
</process:Input>
<process:Input rdf:ID="y">
     <rdfs:label>y</rdfs:label>
     <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/2001/XMLSchema#double</process:parameterType>
</process:Input>
<process:Output rdf:ID="Result">
    <rdfs:label>Result</rdfs:label>
    <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/2001/XMLSchema#double</process:parameterType>
</process:Output>         

<grounding:WsdlGrounding rdf:ID="DivisionGrounding">
	<service:supportedBy rdf:resource="#DivisionService"/>
	<grounding:hasAtomicProcessGrounding rdf:resource="#DivisionWsdlAtomicProcessGrounding"/>
</grounding:WsdlGrounding>


<grounding:WsdlAtomicProcessGrounding rdf:ID="DivisionWsdlAtomicProcessGrounding">
	<grounding:owlsProcess rdf:resource="#DivisionProcess"/>
	<grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Division?wsdl</grounding:wsdlDocument>
	<grounding:wsdlOperation>
            <grounding:WsdlOperationRef>
                <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionOperationsPortType</grounding:portType>
                <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Division</grounding:operation>
            </grounding:WsdlOperationRef>
        </grounding:wsdlOperation>

	<grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionMessage</grounding:wsdlInputMessage>
        <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionResponse</grounding:wsdlOutputMessage>
       
	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#x"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/x</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#y"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/y</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

       <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#Result"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Result</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>
</grounding:WsdlAtomicProcessGrounding>




<grounding:WsdlGrounding rdf:ID="DivisionGrounding2">
	<service:supportedBy rdf:resource="#DivisionService"/>
	<grounding:hasAtomicProcessGrounding rdf:resource="#DivisionWsdlAtomicProcessGrounding2"/>
</grounding:WsdlGrounding>


<grounding:WsdlAtomicProcessGrounding rdf:ID="DivisionWsdlAtomicProcessGrounding2">
	<grounding:owlsProcess rdf:resource="#DivisionProcess"/>
	<grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Division2?wsdl</grounding:wsdlDocument>
	<grounding:wsdlOperation>
            <grounding:WsdlOperationRef>
                <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionOperationsPortType</grounding:portType>
                <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Division</grounding:operation>
            </grounding:WsdlOperationRef>
        </grounding:wsdlOperation>

	<grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionMessage</grounding:wsdlInputMessage>
        <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/DivisionResponse</grounding:wsdlOutputMessage>
       
	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#x"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/x</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#y"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/y</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

       <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#Result"/>
                    <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://localhost:8080/testSFservices/services/Result</grounding:wsdlMessagePart>

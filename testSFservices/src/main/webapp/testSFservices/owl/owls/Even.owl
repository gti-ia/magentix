<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE uridef [
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY service "http://www.daml.org/services/owl-s/1.1/Service.owl#">
  <!ENTITY profile "http://www.daml.org/services/owl-s/1.1/Profile.owl#">
  <!ENTITY process "http://www.daml.org/services/owl-s/1.1/Process.owl#">
  <!ENTITY grounding "http://www.daml.org/services/owl-s/1.1/Grounding.owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema">
  <!ENTITY mind "http://localhost:8080/testSFservices/testSFservices/owl/owls/Even.owl">
  <!ENTITY dir "http://localhost:8080/testSFservices/services/Even?wsdl">
]>


<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:owl="&owl;" xmlns:xsd="&xsd;#" 
   xmlns:service="&service;" xmlns:process="&process;" xmlns:profile="&profile;" xmlns:grounding="&grounding;"
   xmlns:mind="&mind;#"      
   xml:base="http://localhost:8080/testSFservices/testSFservices/owl/owls/Even.owl">

<owl:Ontology>
    <owl:imports rdf:resource="&service;"/>
    <owl:imports rdf:resource="&profile;"/>
    <owl:imports rdf:resource="&process;"/>
    <owl:imports rdf:resource="&grounding;"/> 
</owl:Ontology>

<service:Service rdf:ID="EvenService">
        <service:presents rdf:resource="#EvenProfile"/>
	<service:describedBy rdf:resource="#EvenProcess"/>
	<service:supports rdf:resource="#EvenGrounding"/>
</service:Service>

<profile:Profile rdf:ID="EvenProfile">
    <service:isPresentedBy rdf:resource="#EvenService"/>
    <profile:serviceName xml:lang="en">Even</profile:serviceName>
    <profile:hasInput rdf:resource="#x"/>
    <profile:hasOutput rdf:resource="#Result"/>
</profile:Profile>


<process:AtomicProcess rdf:ID="EvenProcess">
	<service:describes rdf:resource="#EvenService"/>
     	<process:hasInput rdf:resource="#x"/>
	<process:hasOutput rdf:resource="#Result"/>
</process:AtomicProcess>


<process:Input rdf:ID="x">
     <rdfs:label>x</rdfs:label>
     <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#double</process:parameterType>
</process:Input>
<process:Output rdf:ID="Result">
    <rdfs:label>Result</rdfs:label>
    <process:parameterType rdf:datatype="&xsd;#anyURI">&xsd;#boolean</process:parameterType>
</process:Output>         

<grounding:WsdlGrounding rdf:ID="EvenGrounding">
	<service:supportedBy rdf:resource="#EvenService"/>
	<grounding:hasAtomicProcessGrounding rdf:resource="#EvenWsdlAtomicProcessGrounding"/>
</grounding:WsdlGrounding>


<grounding:WsdlAtomicProcessGrounding rdf:ID="EvenWsdlAtomicProcessGrounding">
	<grounding:owlsProcess rdf:resource="#EvenProcess"/>
	<grounding:wsdlDocument rdf:datatype="&xsd;anyURI">http://localhost:8080/testSFservices/services/Even?wsdl</grounding:wsdlDocument>
	<grounding:wsdlOperation>
            <grounding:WsdlOperationRef>
                <grounding:portType rdf:datatype="&xsd;#anyURI">http://localhost:8080/testSFservices/services/EvenOperationsPortType</grounding:portType>
                <grounding:operation rdf:datatype="&xsd;#anyURI">http://localhost:8080/testSFservices/services/Even</grounding:operation>
            </grounding:WsdlOperationRef>
        </grounding:wsdlOperation>

	<grounding:wsdlInputMessage rdf:datatype="&xsd;anyURI">http://localhost:8080/testSFservices/services/EvenMessage</grounding:wsdlInputMessage>
        <grounding:wsdlOutputMessage rdf:datatype="&xsd;anyURI">http://localhost:8080/testSFservices/services/EvenResponse</grounding:wsdlOutputMessage>
       
	<grounding:wsdlInput>
                  <grounding:WsdlInputMessageMap>
                    <grounding:owlsParameter rdf:resource="#x"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/testSFservices/services/x</grounding:wsdlMessagePart>
                  </grounding:WsdlInputMessageMap>
        </grounding:wsdlInput>

       <grounding:wsdlOutput>
                  <grounding:WsdlOutputMessageMap>
		    <grounding:owlsParameter rdf:resource="#Result"/>
                    <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">http://localhost:8080/testSFservices/services/Result</grounding:wsdlMessagePart>
                  </grounding:WsdlOutputMessageMap>
        </grounding:wsdlOutput>
</grounding:WsdlAtomicProcessGrounding>


</rdf:RDF>
